	<trial>
		<name>training</name>
		<reps>100</reps>
		<groups>
			<group>One Layer - Small</group>
			<group>One Layer - Medium</group>
			<group>One Layer - Large</group>
			<group>One Layer - Really Large</group>
			<group>Three Layers</group>
		</groups>
		<initialTasks>
			<task>
				<name>PlaceRobotInitiallyTask</name>
				<params>
					<initialPosition>S</initialPosition>
				</params>
			</task>
			<task>
				<name>ResetModelTask</name>
				<params>
				</params>
			</task>
			<task>
