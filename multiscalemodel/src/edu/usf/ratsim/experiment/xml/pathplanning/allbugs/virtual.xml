	<trial>
		<name>OneWall</name>
		<!-- Groups to apply the trial -->
		<groups>
			<group>Bug0</group>
			<group>Bug1</group>
			<group>Bug2</group>
		</groups>
		<!-- Tasks to perform beforehand -->
		<beforeTrialTasks>
			<task>
				<name>edu.usf.experiment.task.maze.SetUpSingleWall</name>
				<params>
				</params>
			</task>
			<task>
				<name>edu.usf.experiment.task.PublishPlatformPosition</name>
				<params>
				</params>
			</task>
		</beforeTrialTasks>
		<!-- Episodes definition -->
		<episodes>
			<!-- Number of episodes -->
			<number>2</number>
			<!-- Sleep between cycles for visualization -->
			<sleep>10</sleep>
			<!-- Tasks to perform before each episode -->
			<beforeEpisodeTasks>
				<task>
					<name>edu.usf.experiment.task.PlaceRobotInitally</name>
					<params>
						<point>
							<x>.5</x>
							<y>0</y>
							<theta>3.14</theta>
						</point>
					</params>
				</task>
			</beforeEpisodeTasks>
			<!-- Tasks to performe before each cycle -->
			<beforeCycleTasks>
			</beforeCycleTasks>
			<!-- Tasks to performe after each cycle -->
			<afterCycleTasks>
			</afterCycleTasks>
			<!-- Loggers to execute after each cycle -->
			<afterCycleLoggers>
			</afterCycleLoggers>
			<!-- Loggers to execute after each episode -->
			<afterEpisodeLoggers>
				
			</afterEpisodeLoggers>
			<!-- Tasks to execute after each episode -->
			<afterEpisodeTasks>
			</afterEpisodeTasks>
			<!-- Conditions upon which the episode ends -->
			<stopConditions>
				<condition>
					<name>edu.usf.experiment.condition.MaxCycles</name>
					<params>
						<max>5000</max>
					</params>
				</condition>
				<condition>
